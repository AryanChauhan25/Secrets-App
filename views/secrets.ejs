<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">You've Discovered The Secrets!</h1>

    <p class="secret-text">Captain Levi is my hero.</p>
    <% usersWithSecrets.forEach(function(user) { %>
        <% user.secret.forEach(function(i) { %>
            <p class="secret-text"><%=i%><p>
        <% }) %>
    <% }) %>
    <hr>
    
    <a class="btn btn-light btn-lg" href="/login" role="button">Log In</a>
    <a class="btn btn-dark btn-lg" href="/register" role="button">Sign Up</a>
  </div>
</div>

<%- include('partials/footer') %>